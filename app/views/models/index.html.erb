
<div class="container" id="containerb">

<h1>Listing models</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Height</th>
      <th>Size</th>
      <th>Shoesize</th>
      <th>Haircolor</th>
      <th>Eyes</th>
      <th>Bust</th>
      <th>Waist</th>
      <th>Hips</th>
      <th>Phonenumber</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @models.each do |model| %>
      <tr>
        <td><%= model.firstname %></td>
        <td><%= model.lastname %></td>
        <td><%= model.height %></td>
        <td><%= model.size %></td>
        <td><%= model.shoesize %></td>
        <td><%= model.haircolor %></td>
        <td><%= model.eyes %></td>
        <td><%= model.bust %></td>
        <td><%= model.waist %></td>
        <td><%= model.hips %></td>
        <td><%= model.phonenumber %></td>
        <td><%= link_to 'Show', model %></td>
        <td><%= link_to 'Edit', edit_model_path(model) %></td>
        <td><%= link_to 'Destroy', model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Model', new_model_path %>

</div>